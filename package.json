{
  "name"           : "simple-list-mvc",
  "version"        : "0.0.1",
  "description"    : "A dead simple list MVC application",
  "keywords"       : [
    "dead simple list",
    "list mvc",
    "ecosystem:mvc",
    "mvc app"
  ],
  "homepage"       : "https://github.com/dwiyatci/simple-list-mvc",
  "bugs"           : "https://github.com/dwiyatci/simple-list-mvc/issues",
  "license"        : "SEE LICENSE IN LICENSE.txt",
  "author"         : "Glenn Dwiyatcita <dwiyatci@mail.com> (https://tiny.cc/dwiyatci)",
  "repository"     : {
    "type": "git",
    "url" : "https://github.com/dwiyatci/simple-list-mvc.git"
  },
  "scripts"        : {
    "install:global": "npm i -g rimraf updtr webpack webpack-dev-server",
    "install:clean" : "rimraf node_modules && npm i",
    "start"         : "webpack-dev-server --inline --hot",
    "jscs"          : "jscs src",
    "clean"         : "rimraf dist",
    "build"         : "npm run jscs && npm run clean && webpack",
    "update"        : "updtr"
  },
  "dependencies"   : {
    "jquery" : "3.0.0",
    "stampit": "2.1.1"
  },
  "devDependencies": {
    "babel-loader"               : "6.2.4",
    "babel-polyfill"             : "6.9.1",
    "babel-preset-es2015"        : "6.9.0",
    "babel-preset-stage-2"       : "6.5.0",
    "css-loader"                 : "0.23.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-loader"                : "0.4.3",
    "html-webpack-plugin"        : "2.21.0",
    "style-loader"               : "0.13.1",
    "webpack"                    : "1.13.1"
  }
}
